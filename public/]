function vote(serie) {
    fetch('/vote/' + serie)
        .then(function(response) {
            console.log(response);
        });
}

var btns = [].slice.call(document.querySelector('button'));
console.log(btns);
btns.map(function(btn) {
  btn.addEventListener('click', function() {
    alert(this.id);
    vote(this.id);
    console.log(this.id);
  });
});

